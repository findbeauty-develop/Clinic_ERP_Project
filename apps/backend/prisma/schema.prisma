datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Product {
  id         String   @id @default(uuid())
  tenant_id  String
  sku        String
  name       String
  uom        String
  barcode    String?
  is_deleted Boolean  @default(false)
  created_at DateTime @default(now())
  created_by String?
  updated_at DateTime?
  updated_by String?
}

